		tween:Play()
	end
	local transitionInfo = TweenInfo.new(1.5, Enum.EasingStyle.Quint)
	local tween = TweenService:Create(Walkbar, transitionInfo, {Size = UDim2.new(0,0,1,0)})
	tween:Play()
